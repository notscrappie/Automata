{
  "name": "@shadowrunners/automata",
  "version": "2.3.2",
  "description": "Automata is a fork of the Poru (by parasop) lavalink client developed and maintained by scrappie.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "localdeploy": "npm run build && npm link"
  },
  "tsup": {
    "dts": true,
    "bundle": false,
    "treeshake": true,
    "target": "node16",
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "src/**/*.ts",
      "index.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadowrunners/Automata.git"
  },
  "author": "Original Author: parasop | Forked by scrappie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shadowrunners/Automata/issues"
  },
  "homepage": "https://github.com/shadowrunners/Automata#readme",
  "dependencies": {
    "undici": "^5.22.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
